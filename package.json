{
  "name": "basic-fsa-factories",
  "version": "1.0.0",
  "description": "basic (0.3kB) partial implementation of some common flux standard action (FSA) factories to instantiate action factories and dispatchers",
  "main": "dist/index.min.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "predist": "mkdirp dist && rimraf dist/*",
    "dist": "tsc -p src --outDir dist",
    "postdist": "npm run uglify",
    "prepublish": "npm run dist",
    "pretest": "npm run dist",
    "test": "jasmine",
    "test:web:serve": "http-server -c-1 -o -p 6510 spec/web",
    "pretest:web": "rimraf spec/web/*.js && npm run pretest",
    "test:web": "browserify spec/*.spec.js -g uglifyify -p bundle-collapser/plugin -p [ common-shakeify -v ] | uglifyjs -c -m toplevel -o spec/web/index.spec.js && npm run test:web:serve",
    "uglify": "uglifyjs dist/index.js -c -m toplevel -o dist/index.min.js"
  },
  "keywords": [
    "action",
    "fsa",
    "react",
    "preact",
    "inferno",
    "redux",
    "reducer"
  ],
  "author": {
    "name": "Stephane M. Catala",
    "email": "stephane@zenyway.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenyway/basic-fsa-factories.git"
  },
  "bugs": {
    "url": "https://github.com/zenyway/basic-fsa-factories/issues"
  },
  "homepage": "https://github.com/zenyway/basic-fsa-factories#readme",
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "browserify": "^16.2.2",
    "bundle-collapser": "^1.3.0",
    "common-shakeify": "^0.4.6",
    "http-server": "^0.11.1",
    "jasmine": "^3.1.0",
    "rimraf": "^2.6.2",
    "tsify": "^4.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "uglifyify": "^5.0.0"
  },
  "dependencies": {}
}
